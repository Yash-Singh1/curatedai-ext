{
  "name": "CuratedAI",
  "version": "1.0",
  "description": "Catored high-quality search results",
  "background": {
    "matches": ["<all_urls>"],
    "all_frames": true,
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["declarativeContent", "activeTab", "storage"],
  "manifest_version": 2,
  "browser_action": {
    "default_icon" : "img/extension_icon.png",
    "default_title": "Create Bookmark"
  },
  "icons": {
    "128": "img/extension_icon.png"
  }
}
